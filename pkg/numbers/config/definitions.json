    "id": "blockchain-numbers",
    "public": true,
    "spec": {
      "resource_specification": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Numbers Protocol Blockchain Connector Spec",
        "type": "object",
        "required": [
          "capture_token"
        ],
        "additionalProperties": false,
        "properties": {
          "capture_token": {
            "title": "Capture token",
            "description": "Fill your Capture token in the Capture App. To access your tokens, you need a Capture App account and you can sign in with email or wallet to acquire the Capture Token.",
            "type": "string",
            "credential_field": true
          }
        }
      },
      "component_specification": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Numbers Protocol Component Setting",
        "type": "object",
        "properties": {
          "metadata": {
            "title": "Metadata",
            "type": "object"
          },
          "input": {
            "type": "object",
            "properties": {
              "images": {
                "title": "Images",
                "description": "The images you want to upload to blockchain",
                "instillFormat": "image_array",
                "instillUpstreamTypes": [
                  "reference"
                ],
                "anyOf": [
                  {
                    "type": "string",
                    "pattern": "^\\{.*\\}$",
                    "instillUpstreamType": "reference"
                  }
                ]
              },
              "asset_creator": {
                "title": "Asset Creator",
                "description": "Name of the asser creator",
                "instillFormat": "text",
                "instillUpstreamTypes": [
                  "value",
                  "reference"
                ],
                "anyOf": [
                  {
                    "type": "string",
                    "instillUpstreamType": "value"
                  },
                  {
                    "type": "string",
                    "pattern": "^\\{.*\\}$",
                    "instillUpstreamType": "reference"
                  }
                ]
              },
              "abstract": {
                "title": "Abstract",
                "description": "A summary or abstract of the asset",
                "instillFormat": "text",
                "instillUpstreamTypes": [
                  "value",
                  "reference"
                ],
                "anyOf": [
                  {
                    "type": "string",
                    "instillUpstreamType": "value"
                  },
                  {
                    "type": "string",
                    "pattern": "^\\{.*\\}$",
                    "instillUpstreamType": "reference"
                  }
                ]
              },
              "custom": {
                "title": "Custom",
                "type": "object",
                "properties": {
                  "digital_source_type": {
                    "title": "Digital Source Type",
                    "description": "Specify the type of the source. More details see https://docs.numbersprotocol.io/introduction/numbers-protocol/defining-web3-assets/assettree/digitalsourcetype",
                    "instillFormat": "text",
                    "instillUpstreamTypes": [
                      "value",
                      "reference"
                    ],
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "trainedAlgorithmicMedia",
                          "trainedAlgorithmicData",
                          "digitalCapture",
                          "digitalArt",
                          "algorithmicMedia"
                        ],
                        "default": "trainedAlgorithmicMedia",
                        "instillUpstreamType": "value"
                      },
                      {
                        "type": "string",
                        "pattern": "^\\{.*\\}$",
                        "instillUpstreamType": "reference"
                      }
                    ]
                  },
                  "mining_preference": {
                    "title": "Mining Preference",
                    "description": "Designates the selection made by the asset creators or licensed owners to decide if the asset is suitable for inclusion in a data mining or AI/ML training workflow. More details see https://docs.numbersprotocol.io/introduction/numbers-protocol/defining-web3-assets/assettree/miningpreference",
                    "instillFormat": "text",
                    "instillUpstreamTypes": [
                      "value",
                      "reference"
                    ],
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "dataMining",
                          "aiInference",
                          "notAllowed",
                          "aiGenerativeTraining",
                          "aiGenerativeTrainingWithAuthorship",
                          "aiTraining",
                          "aiTrainingWithAuthorship"
                        ],
                        "default": "notAllowed",
                        "instillUpstreamType": "value"
                      },
                      {
                        "type": "string",
                        "pattern": "^\\{.*\\}$",
                        "instillUpstreamType": "reference"
                      }
                    ]
                  },
                  "generated_through": {
                    "title": "Generated Through",
                    "description": "URL of the service that is used to generate the content",
                    "instillFormat": "text",
                    "instillUpstreamTypes": [
                      "value",
                      "reference"
                    ],
                    "anyOf": [
                      {
                        "type": "string",
                        "default": "https://console.instill.tech",
                        "instillUpstreamType": "value"
                      },
                      {
                        "type": "string",
                        "pattern": "^\\{.*\\}$",
                        "instillUpstreamType": "reference"
                      }
                    ]
                  },
                  "generated_by": {
                    "title": "Generated By",
                    "description": "The AI model used to generate the content",
                    "instillFormat": "text",
                    "instillUpstreamTypes": [
                      "value",
                      "reference"
                    ],
                    "anyOf": [
                      {
                        "type": "string",
                        "instillUpstreamType": "value"
                      },
                      {
                        "type": "string",
                        "pattern": "^\\{.*\\}$",
                        "instillUpstreamType": "reference"
                      }
                    ]
                  },
                  "creator_wallet": {
                    "title": "Creator Wallet",
                    "description": "the Wallet address of the asset creator",
                    "instillFormat": "text",
                    "instillUpstreamTypes": [
                      "value",
                      "reference"
                    ],
                    "anyOf": [
                      {
                        "type": "string",
                        "instillUpstreamType": "value"
                      },
                      {
                        "type": "string",
                        "pattern": "^\\{.*\\}$",
                        "instillUpstreamType": "reference"
                      }
                    ]
                  },
                  "license": {
                    "title": "License",
                    "type": "object",
                    "properties": {
                      "name": {
                        "title": "License Name",
                        "description": "License of the asset file",
                        "instillFormat": "text",
                        "instillUpstreamTypes": [
                          "value",
                          "reference"
                        ],
                        "anyOf": [
                          {
                            "type": "string",
                            "instillUpstreamType": "value"
                          },
                          {
                            "type": "string",
                            "pattern": "^\\{.*\\}$",
                            "instillUpstreamType": "reference"
                          }
                        ]
                      },
                      "document": {
                        "title": "License Document",
                        "description": "URL of the license file",
                        "instillFormat": "text",
                        "instillUpstreamTypes": [
                          "value",
                          "reference"
                        ],
                        "anyOf": [
                          {
                            "type": "string",
                            "instillUpstreamType": "value"
                          },
                          {
                            "type": "string",
                            "pattern": "^\\{.*\\}$",
                            "instillUpstreamType": "reference"
                          }
                        ]
                      }
                    }
                  }
                }
              }
            },
            "title": "Data Flow"
          }
        }
